{
  "name": "onLink",
  "version": "0.5.1",
  "engines": {
    "node": "6.x"
  },
  "description": "onLink",
  "main": "index.js",
  "scripts": {
    "installClient": "cd client && npm install",
    "watchClient": "cd client && npm run build -- --watch",
    "buildClient": "cd client && npm run build",
    "installServer": "cd server && npm install",
    "watchServer": "cd server && npm run watch",
    "startServer": "cd server && npm run start",
    "startServices": "node services/startAll.js",
    "postinstall": "run-p installClient installServer",
    "start": "run-s startServices buildClient startServer",
    "dev": "run-p startServices watchClient watchServer",
    "watch": "run-p startServices watchClient watchServer"
  },
  "repository": {
    "type": "git"
  },
  "author": "zhenchao.chen",
  "dependencies": {
    "async": "2.0.0-rc.5",
    "npm-run-all": "^2.1.0"
  }
}
